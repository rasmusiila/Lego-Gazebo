	  <mesh>
            <uri>model://lego_mindstorm_camera/meshes/body.dae</uri>
          </mesh>
        </geometry>
      </collision>

      <visual name='visual'>
        <geometry>
	  <!--box>
	    <size>0.09 0.18 .118</size>
	  </box-->
	  <mesh>
            <uri>model://lego_mindstorm_camera/meshes/body.dae</uri>
          </mesh>
