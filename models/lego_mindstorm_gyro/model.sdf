	  <box>
	    <size>0.02 0.04 .02</size>
	  </box>
        </geometry>
      </visual>
    </link>

    <link name="left_wheel">
      <pose>-0.059 0.032 0.028 0 1.5707 0</pose>
      <collision name="collision">
	<!--pose>-0.028 0 0 0 1.5707 0</pose-->
        <geometry>
	  <cylinder>
            <radius>.028</radius>
            <length>.029</length>
          </cylinder>
          <!--mesh>
            <uri>model://lego_mindstorm_camera/meshes/wheel.dae</uri>
          </mesh-->
        </geometry>
	<surface>
      	  <friction>
	    <ode>
  	      <mu>0.6</mu>
	      <mu2>0.6</mu2>
	    </ode>
          </friction>
    	</surface>
      </collision>

	<inertial>
	 <mass>1</mass>
	 <inertia>
	 <ixx>0.00066</ixx>
	 <iyy>0.00066</iyy>
	 <izz>0.0005</izz>
	 </inertia>
	 </inertial>

      <visual name="visual">
