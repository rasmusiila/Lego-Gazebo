	  <box>
	    <size>0.016 0.016 0.016</size>
	  </box>
	  <!--mesh>
            <uri>model://lego_mindstorm_camera/meshes/caster.dae</uri>
          </mesh-->
    	</geometry>

    	<surface>
      	  <friction>
	    <ode>
	    <mu>0</mu>
	    <mu2>0</mu2>
	    <slip1>1.0</slip1>
	    <slip2>1.0</slip2>
	    </ode>
	  </friction>
    	</surface>
      </collision>

      <visual name='caster_visual'>
    	<pose>0 -0.0777713 0 0 0 0</pose>
    	<geometry>
	  <mesh>
            <uri>model://lego_mindstorm_camera/meshes/caster.dae</uri>
